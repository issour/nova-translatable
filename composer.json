{
    "name": "xxc/nova-translatable",
    "description": "A Laravel Nova field for spatie/laravel-translatable.",
    "keywords": [
        "laravel",
        "nova",
        "translatable"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "spatie/laravel-translatable": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "xXc\\Translatable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "xXc\\Translatable\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
